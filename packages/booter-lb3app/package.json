{
  "name": "@loopback/booter-lb3app",
  "version": "1.0.0",
  "description": "Boot a LoopBack 3 application in LoopBack 4 project",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build:apidocs": "lb-apidocs",
    "build": "lb-tsc es2017 --outDir dist",
    "clean": "lb-clean loopback-booter-lb3app*.tgz dist package api-docs",
    "pretest": "npm run build",
    "test": "lb-mocha \"dist/__tests__/**/*.js\"",
    "verify": "npm pack && tar xf loopback-booter-lb3app*.tgz && tree package && npm run clean"
  },
  "author": "IBM",
  "copyright.owner": "IBM Corp.",
  "license": "MIT",
  "dependencies": {
    "@types/debug": "^4.1.3",
    "@types/express": "^4.16.1",
    "debug": "^4.1.1",
    "loopback-swagger": "^5.7.2",
    "p-event": "^4.1.0",
    "swagger2openapi": "^5.2.5"
  },
  "peerDependencies": {
    "@loopback/boot": "^1.1.2",
    "@loopback/core": "^1.2.1",
    "@loopback/rest": "^1.9.1"
  },
  "devDependencies": {
    "@loopback/boot": "^1.1.2",
    "@loopback/core": "^1.2.1",
    "@loopback/build": "^1.4.0",
    "@loopback/rest": "^1.9.1",
    "@loopback/testlab": "^1.2.1",
    "@loopback/tslint-config": "^2.0.3",
    "@types/node": "^10.11.2"
  },
  "keywords": [
    "LoopBack",
    "LB3",
    "Boot",
    "Compatibility"
  ],
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/strongloop/loopback-next.git"
  }
}
